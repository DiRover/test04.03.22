{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noEmit": true,
        "noImplicitAny": true,
        "allowJs": false,
        "module": "esnext",
        "target": "es6",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
